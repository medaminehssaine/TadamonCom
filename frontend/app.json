{
  "expo": {
    "name": "TadamonCom",
    "slug": "tadamoncom",
    "version": "1.0.0",
    "sdkVersion": "52.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TadamonLogo.png",
    "scheme": "Tadamoncom",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "android", "web"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adarissa.tadamoncom"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TadamonLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adarissa.tadamoncom"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/TadamonLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/TadamonLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
